<%- include('../partials/header') %>

<h3><b>User List</b></h3>
<% users.forEach(u => { %>
  <a href="/users/<%= u._id %>">
    <div class="card">
      <div class="card-body">
        <img height="50" id="avatarPhoto" src="<%= u.avatar %>" alt=""> 
        <%= u.name %></a> - Joined on <%= u.createdAt.toLocaleString().split(',')[0] %>
      </div>
    </div><br>
<% }) %>


<%- include('../partials/footer') %>